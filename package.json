{
  "name": "@iroomit/avataaars",
  "version": "2.2.0",
  "description": "React component for avataaars",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc"
  },
  "keywords": [
    "react",
    "avatar"
  ],
  "repository": "git@github.com:fangpenlin/avataaars.git",
  "author": "Fang-Pen Lin <hello@fangpenlin.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "import-sort-style-renke": "^4.2.0",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0",
    "tslint-react": "^5.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "lodash": ">=4.17.21",
    "prop-types": "^15.7.2"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
